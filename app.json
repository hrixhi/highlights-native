{
    "expo": {
        "name": "Cues - Notebook",
        "slug": "Cues",
        "version": "1.0.1",
        "icon": "./assets/images/icon.png",
        "scheme": "cues",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#2f2f3c"
        },
        "updates": {
            "fallbackToCacheTimeout": 30000
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.vora.cues",
            "buildNumber": "1.5",
            "usesIcloudStorage": true
        },
        "android": {
            "useNextNotificationsApi": true,
            "versionCode": 4,
            "softwareKeyboardLayoutMode": "pan",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#2f2f3c"
            },
            "package": "com.vora.cues",
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": ["js", "jsx", "css"],
            "transformer": "node_modules/react-native-css-transformer/index.js"
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to help you create notes and content.",
                    "cameraPermission": "The app accesses your camera to help you create notes and content."
                }
            ]
        ]
    }
}
